{
  "manifest_version": 3,
  "name": "YouTube Shorts Blocker",
  "version": "1.0.0",
  "description": "YouTubeのショート動画を非表示にするChrome拡張機能",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["*://www.youtube.com/*"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ]
}